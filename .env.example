# App Configuration
NEXT_PUBLIC_APP_NAME=LelangMobil
NEXT_PUBLIC_APP_URL=http://localhost:3000

# API Configuration (untuk production)
# NEXT_PUBLIC_API_URL=https://api.lelangmobil.com
# API_SECRET_KEY=your-secret-key-here

# Database (untuk production)
# DATABASE_URL=postgresql://user:password@localhost:5432/lelangmobil

# Upload Configuration (untuk production)
# NEXT_PUBLIC_UPLOAD_URL=https://cdn.lelangmobil.com
# UPLOAD_MAX_SIZE=5242880

# Email Configuration (untuk production)
# SMTP_HOST=smtp.gmail.com
# SMTP_PORT=587
# SMTP_USER=your-email@gmail.com
# SMTP_PASS=your-app-password

# Payment Gateway (untuk production)
# MIDTRANS_SERVER_KEY=your-midtrans-server-key
# MIDTRANS_CLIENT_KEY=your-midtrans-client-key
# MIDTRANS_IS_PRODUCTION=false

# Redis (untuk production)
# REDIS_URL=redis://localhost:6379

# AWS S3 (untuk production)
# AWS_ACCESS_KEY_ID=your-access-key
# AWS_SECRET_ACCESS_KEY=your-secret-key
# AWS_REGION=ap-southeast-1
# AWS_BUCKET_NAME=lelangmobil-uploads

# Analytics
# NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX

# Feature Flags
# NEXT_PUBLIC_ENABLE_CHAT=false
# NEXT_PUBLIC_ENABLE_VIDEO_CALL=false
